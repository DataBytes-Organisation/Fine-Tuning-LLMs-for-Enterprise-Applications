    "                    return \"Neutral\"\n",
    "                return sentiment  # Success!\n",
    "            except (json.JSONDecodeError, TypeError):\n",
    "                return \"Neutral\"  # Parsing failed\n",
    "\n",
    "        except ResourceExhausted as e:  # Specific error for 429\n",
